{
  "version": 2,
  "buildCommand": "npm run build",
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x",
      "includeFiles": [
        "api/server/**",
        "api/client/**"
      ]
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/api/client/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}



